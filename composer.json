{
    "name": "barbieswimcrew/dynamic-form-bundle",
    "type": "symfony-bundle",
    "description": "Symfony form component extensions for dynamic modification of forms via JS and user submitted data",
    "keywords": ["symfony", "symfony2", "symfony3", "form", "validation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Schindler"
        },
        {
            "name": "Anton Zoffmann"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/form": "2.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*",
        "symfony/validator": "2.7.*",
        "symfony/dependency-injection": "2.7.*"
    },
    "autoload": {
        "psr-4": {
            "Barbieswimcrew\\Bundle\\DynamicFormBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Barbieswimcrew\\Bundle\\DynamicFormBundle\\Tests\\": "Tests/"
        }
    }
}
